menu "LPC11Axx_PIO0_0"	

config LPC11Axx_PIO0_0_DEFAULT
	bool "default"
	default y

choice
	prompt "FUNCTION"
	default LPC11Axx_PIO0_0_RESET
	depends on !LPC11Axx_PIO0_0_DEFAULT
	depends on !LPC11Axx_PIO0_0_DEFAULT

config LPC11Axx_PIO0_0_RESET
    bool	"RESET"

config LPC11Axx_PIO0_0_GPIO
    bool	"GPIO"

endchoice

config LPC11xx_PIO0_0_FUNCTION
    int
    default 0 if LPC11Axx_PIO0_0_RESET
    default 1 if LPC11Axx_PIO0_0_GPIO
  

choice
	prompt "MODE"	
	default LPC11Axx_PIO0_0_PULLUP
	depends on !LPC11Axx_PIO0_0_DEFAULT

config LPC11Axx_PIO0_0_PULLUP
	bool	"PullUp"

config LPC11Axx_PIO0_0_REPEATER
	bool	"Repeater Mode"

config LPC11Axx_PIO0_0_NO
	bool	"Neither PullUp nor PullDown"
    
config LPC11Axx_PIO0_0_PULLDOWN
	bool	"PullDown"

endchoice

config LPC11xx_PIO0_0_MODE
	int
	default 0 if LPC11Axx_PIO_0_PULLUP
	default 8 if LPC11Axx_PIO0_0_REPEATER
	default 16 if LPC11Axx_PIO0_0_NO
	default 24 if LPC11Axx_PIO0_0_PULLDOWN

config LPC11Axx_PIO0_0_OPENDRAIN_ENABLE
    	bool "OPENDRAIN"
	default n
	depends on !LPC11Axx_PIO0_0_DEFAULT
    
config LPC11xx_PIO0_0_OPENDRAIN
    	int
	default 0 if !LPC11Axx_PIO0_0_OPENDRAIN_ENABLE
	default 1 if LPC11Axx_PIO0_0_OPENDRAIN_ENABLE


endmenu

