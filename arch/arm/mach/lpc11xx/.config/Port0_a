	depends on !LPC11Axx_PIO0_31_DEFAULT

config LPC11Axx_PIO0_31_PULLUP
	bool	"PullUp"

config LPC11Axx_PIO0_31_REPEATER
	bool	"Repeater Mode"

config LPC11Axx_PIO0_31_NO
	bool	"Neither PullUp nor PullDown"
    
config LPC11Axx_PIO0_31_PULLDOWN
	bool	"PullDown"

endchoice

config LPC11xx_PIO0_31_MODE
	int
	default 0 if LPC11Axx_PIO0_31_PULLUP
	default 8 if LPC11Axx_PIO0_31_REPEATER
	default 16 if LPC11Axx_PIO0_31_NO
	default 24 if LPC11Axx_PIO0_31_PULLDOWN

config LPC11Axx_PIO0_31_OPENDRAIN_ENABLE
    	bool "OPENDRAIN"
	default n
	depends on !LPC11Axx_PIO0_31_DEFAULT
    
config LPC11xx_PIO0_31_OPENDRAIN
    	int
	default 0 if !LPC11Axx_PIO0_31_OPENDRAIN_ENABLE
	default 1 if LPC11Axx_PIO0_31_OPENDRAIN_ENABLE

endmenu
